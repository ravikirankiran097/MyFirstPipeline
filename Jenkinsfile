pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..1'
                sleep 15
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..1'
                sleep 15
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....1'
            }
        }
    }
}
